        "postcss": "^8.1.14",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "vue": "^2.6.12",
        "vue-loader": "^15.9.8",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "@mdi/font": "^6.5.95",
        "chart.js": "^2.9.4",
        "material-design-icons-iconfont": "^6.1.1",
        "vue-chartjs": "^3.5.1",
